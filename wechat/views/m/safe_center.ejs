<!doctype html>
<html lang="">
<%- include('../common/head', {title: '安全中心'}) %>
<body class="wechat">
  <!--1-->
  <div class="wechat-myac-safecy">
    <section class="wechat-myac-safecolumn bd-bottom">
      <span class="wechat-myac-approve">
        <% if(data.userstatusmap.isSetNickName) {%>
          <%=  data.center.nickName.substring(0, 1) + ' ****** ' + data.center.nickName.substring(data.center.nickName.length -1 , data.center.nickName.length) %>
        <% } else {%>
          <%=  data.center.mobile.substring(0, 3) + ' ***** ' + data.center.mobile.substring(8, 11) %>
        <% }%>
      </span>
      <span>昵称</span>
    </section>
    <section class="wechat-myac-safecolumn bd-bottom">
      <% if(data.userstatusmap.secretLevel <= 2) {%>
        <span class="wechat-myac-approve level-low">低</span>
      <% } else if (data.userstatusmap.secretLevel > 5) {%>
        <span class="wechat-myac-approve level">高</span>
      <% } else {%>
        <span class="wechat-myac-approve level-low">中</span>
      <% }%>
      <span>安全等级</span>
    </section>
    <section class="wechat-myac-safecolumn">
      <% if(data.userstatusmap.id5Status === '0') {%>
      <span class="wechat-myac-action">
        <a href="/m/id5_auth"> <em>未设置</em> <i class="wechat-myac-point-ico">&nbsp;</i></a>
      </span>
      <% }%>
      <% if (data.userstatusmap.id5Status === '1') {%>
      <span class="wechat-myac-approve">审核中</span>
      <% }%>
      <% if (data.userstatusmap.id5Status === '2') {%>
      <span class="wechat-myac-action">
        <a href="javascript:void(0)"> <em>审核未通过</em> <i class="wechat-myac-point-ico">&nbsp;</i></a>
      </span>
      <% }%>
      <% if (data.userstatusmap.id5Status === '3') {%>
      <span class="wechat-myac-approve"><%= data.center.realName.substring(0, 1) + ' **'%></span>
      <% }%>
      <span>实名认证</span>
    </section>
  </div>
